   "source": [
    "import itertools\n",
    "for j in itertools.product(range(2), repeat=5):\n",
    "    if all(i==0 for i in j):\n",
    "        continue\n",
    "    i = min(i1 for i1 in range(5) if j[i1] != 0)\n",
    "    print(j)\n",
    "    print(i)\n",
    "    print('------')"
   ]
